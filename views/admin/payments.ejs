                    <th>Date</th>
                    <th>Transaction</th>
                    <th>User ID</th>
                    <th>Service ID</th>
                    <th>Direction</th>
                    <th>Amount</th>
                </tr>
            </thead>
            <tbody data-bind="foreach: transactions">
                <tr>

                    <!--self.serviceId = ko.observable(data.servicesID);-->
