<div class="container">
    <script src="/scripts/interface.js"></script>
    <script src="http://momentjs.com/downloads/moment-with-locales.js"></script>
	<div class="row search_line">
		<div class="col s12 m12">
			<div class="card card_methods teal lighten-1">
				<div class="card-content client_payment white-text">
					<h5 class="header section-title">
                        <span><i class="small material-icons">language </i>
                                Proxy Browsing
                        </span>
					</h5>
				</div>
				<div class="card-action card-action-line has-browser">


					<!-- <div class="col m1 s12 search_button center-align ">
                        <nav>
                            <ul class="left center">
                                <li><a href="#"><i class="mdi-content-undo small"></i></a></li>
