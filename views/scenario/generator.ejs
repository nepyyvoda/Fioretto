<div class="container">
    <div class="row">
        <div class="col s12 m12">
            <div class="admin-panel">
                <div class="row">
                    <div class="col m7 s7">
                        <div class="row col m12 s12">
                            <div class="col m4 s4 form-group">
                                <label for="name">Email address</label>
                                <input placeholder="Name" name="name" type="text" class="form-control" >
                            </div>
                            <div class="col m4 s4 form-group">
                                <label for="count">Email address</label>
                                <input placeholder="Votes" name="count" type="text" class="form-control" >
                            </div>
                            <div class="col m4 s4">
                                <button id="clearChain" class="btn btn-default" type="button">Clear chain</button>
                                <button id="accept-voting" class="btn btn-primary" type="button">Save voting</button>
                            </div>
                        </div>
                    </div>
                    <div class="col m5 s5">
                        <div>
                            <button id="captchaActivate" class="btn btn-warning btn-captcha">CAPTCHACHEKER</button>
                        </div>
                        <div>
                            <button id="captchaImage" class="btn btn-sm btn-captcha disabled">CAPTCHAIMG</button>
                            <label id="captchaImageStatus" class="hide">Image not selected</label>
                        </div>
                        <div>
                            <button id="captchaInput" class="btn btn-sm btn-captcha disabled">CAPTCHAINPUT</button>
                            <label id="captchaInputStatus" class="hide">Input not selected</label>
                        </div>
                    </div>
                </div>
            </div>
            <iframe src="/api<%= proxyUrl %>" width="100%" height="4000" frameborder=0 scrolling="no" style="position: absolute; width: 80%;" ></iframe>
        </div>
    </div>
</div>

<script src="/scripts/iframe.js"></script>